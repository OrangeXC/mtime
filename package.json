{
  "name": "mtime",
  "version": "2.0.0",
  "description": "mtime",
  "private": true,
  "dependencies": {
    "egg": "^2.11.0",
    "egg-mysql": "^3.0.0",
    "egg-scripts": "^2.9.1",
    "egg-sequelize": "^4.1.0",
    "egg-view-nunjucks": "^2.2.0",
    "mockjs": "^1.0.1-beta3",
    "mysql2": "^1.6.1",
    "mz": "^2.7.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.8.5",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.20.0",
    "eslint": "^5.5.0",
    "eslint-config-egg": "^7.1.0",
    "typescript": "^3.0.3",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon",
    "stop": "egg-scripts stop",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "migrate:new": "egg-sequelize migration:create",
    "migrate:up": "egg-sequelize db:migrate",
    "migrate:down": "egg-sequelize db:migrate:undo"
  },
  "ci": {
    "version": "8, 9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OrangeXC/mtime"
  },
